						$('#slide-centros').addClass("loaded");
					}
					
				});
			});
		});
		
	});

	$('#link3').click(function(){
		$("#galeria-titulo").html("Tiendas y Espacios Comerciales<br/>");
		$("#galeria-titulo").append('<span style="font-size:15px;" >Espacios comerciales menores</span>');

		
		

		$('#slide-centros').fadeOut(500,function(){
			$("#slide-tiendas").fadeOut(500,function(){
