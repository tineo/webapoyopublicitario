					if( $('#slide-espacios').is('.loaded') ){ }else{
						$('#slide-espacios').camera({
							height: '400px',
							loader: 'bar',
							pagination: false,
							thumbnails: true,
							time: 4000,
							transPeriod: 1000
						});
						$('#slide-espacios').addClass("loaded");

					}
				});
			});
		});
		
	});




	
});
</script>


